<?xml version="1.0" encoding="windows-1251"?>
<!DOCTYPE hrd PUBLIC "-//Cail Lomecb//DTD Colorer HRD take5//EN"
  "http://colorer.sf.net/2003/hrd.dtd">
<hrd xmlns="http://colorer.sf.net/2003/hrd">
  <!-- text colors -->
  <assign name="xce:TextColor" cfore="#0" cback="#7" fore="#000000" back="#c0c0c0" />
  <assign name="xce:CurrentLineTextColor" cfore="#1" cback="#7" fore="#c00000" back="#c0c0c0" />

  <assign name="xce:BlockTextColor" cfore="#0" cback="#3" fore="#000000" back="#e6a000" />
  <assign name="xce:CurrentLineBlockTextColor" cfore="#1" cback="#3" fore="#c00000" back="#e6a000"/>

  <assign name="xce:SpellCheckErrorColor" cfore="#4" cback="#7" fore="#d30094" back="#c0c0c0" style="4" />
  <assign name="xce:PairHighlightColor" cfore="#2" cback="#7" fore="#328e6b" back="#c0c0c0" />
  <assign name="xce:SearchHighlight" cfore="#7" cback="#1" fore="#000000" back="#face87" />


  <assign name="def:Text" cfore="#0" cback="#7" fore="#000000" back="#c0c0c0"/>
  <assign name="def:Syntax" cfore="#0" cback="#7" fore="#000000" back="#c0c0c0"/>

  <assign name="def:Number"    cfore="#2"  cback="#7" fore="#006400" back="#c0c0c0"/>
  <assign name="def:NumberSuffix" cfore="#A"  cback="#7"  fore="#008000" back="#c0c0c0"/>

  <assign name="def:String" cfore="#1"  cback="#7"  fore="#cd0000" back="#c0c0c0"/>
  <assign name="def:StringContent" cfore="#1"  cback="#7"  fore="#cd0000" back="#c0c0c0"/>
  <assign name="def:StringEdge" cfore="#1"  cback="#7"  fore="#cd0000" back="#c0c0c0"/>
  <assign name="def:Character" cfore="#1"  cback="#7"  fore="#cd0000" back="#c0c0c0"/>
  <assign name="def:CharacterContent" cfore="#1"  cback="#7"  fore="#cd0000" back="#c0c0c0"/>

  <assign name="def:Comment" cfore="#8"  cback="#7"  fore="#909090" back="#c0c0c0"/>
  <assign name="def:CommentContent" cfore="#1"  cback="#7"  fore="#909090" back="#c0c0c0"/>
  <assign name="def:CommentEdge"    cfore="#8"  cback="#7"  fore="#909090" back="#c0c0c0"/>
  <assign name="def:CommentDoc"     cfore="#8"  cback="#7"  fore="#909090" back="#c0c0c0"/>
  <assign name="def:CommentDocContent" cfore="#8"  cback="#7"  fore="#909090" back="#c0c0c0"/>
  <assign name="def:CommentDocEdge" cfore="#8"  cback="#7"  fore="#909090" back="#c0c0c0"/>

  <assign name="def:Symbol" cfore="#1"  cback="#7"  fore="#e16941" back="#c0c0c0"/>
  <assign name="def:SymbolStrong" cfore="#8"  cback="#7"  fore="#e16941" back="#c0c0c0"/>
  <assign name="def:Prefix" cfore="#1"  cback="#7"  fore="#e16941" back="#c0c0c0"/>

  <assign name="def:Operator" cfore="#1"  cback="#7"  fore="#000000" back="#c0c0c0"/>

  <assign name="def:Keyword"         cfore="#8"  cback="#7"  fore="#606060" back="#c0c0c0" style="1" />
  <assign name="def:TypeKeyword"   cfore="#8"  cback="#7"  fore="#606060" back="#c0c0c0"/>

  <assign name="def:Directive" cfore="#9"  cback="#7"  fore="#578b2e" back="#c0c0c0"/>

  <assign name="def:Tag" cfore="#0"  cback="#7"  fore="#000000" back="#c0c0c0"/>
  <assign name="def:OpenTag" cfore="#1"  cback="#7"  fore="#ff901e" back="#c0c0c0"/>
  <assign name="def:CloseTag" cfore="#1"  cback="#7"  fore="#ff901e" back="#c0c0c0"/>

  <assign name="def:Error" cfore="#F" cback="#4"  fore="#000000" back="#2a2aa5"/>
  <assign name="def:ErrorText" cfore="#D"  cback="#7"  fore="#9314ff" back="#c0c0c0"/>

  <!-- UI colors -->
  <assign name="xce:StatusLineColor" cfore="#0" cback="#7" fore="#ffffff" back="#606060" />

  <assign name="xce:MenuItem" cfore="#f" cback="#8" fore="#ffffff" back="#606060" />
  <assign name="xce:MenuHotKey" cfore="#E" cback="#8" fore="#00c0c0" back="#606060" style="4" />
  <assign name="xce:MenuDisabledItem" cfore="#7" cback="#8" fore="#c0c0c0" back="#606060" />

  <assign name="xce:MenuItemSelected" cfore="#f" cback="#7" fore="#ffffff" back="#ff8000" />
  <assign name="xce:MenuHotKeySelected" cfore="#E" cback="#7" fore="#00ffff" back="#ff8000" style="4" />
  <assign name="xce:MenuDisabledItemSelected" cfore="#8" cback="#7" fore="#c0c0c0" back="#ff8000" />

  <assign name="xce:DialogBorder" cfore="#0" cback="#8" fore="#ffffff" back="#606060" />

  <assign name="xce:DialogItemLabelColor" cfore="#0" cback="#8" fore="#000000" back="#606060" />
  <assign name="xce:DialogItemLabelHotKey" cfore="#E" cback="#8" fore="#00c0c0" back="#606060" style="4" />

  <assign name="xce:DialogItemCheckBoxColor" cfore="#0" cback="#8" fore="#000000" back="#606060" />
  <assign name="xce:DialogItemCheckBoxColorFocused" cfore="#f" cback="#8" fore="#ffffff" back="#606060" />
  <assign name="xce:DialogItemCheckBoxHotKey" cfore="#e" cback="#8" fore="#00c0c0" back="#606060" style="4" />
  <assign name="xce:DialogItemCheckBoxHotKeyFocused" cfore="#e" cback="#8" fore="#00ffff" back="#606060" style="4" />
  <assign name="xce:DialogItemCheckBoxColorDisabled" cfore="#7" cback="#8" fore="#c0c0c0" back="#606060" />

  <assign name="xce:DialogItemButtonColor" cfore="#0" cback="#8" fore="#000000" back="#606060" />
  <assign name="xce:DialogItemButtonColorFocused" cfore="#f" cback="#8" fore="#ffffff" back="#606060" style="1" />
  <assign name="xce:DialogItemButtonHotKey" cfore="#e" cback="#8" fore="#00c0c0" back="#606060" style="4" />
  <assign name="xce:DialogItemButtonHotKeyFocused" cfore="#e" cback="#8" fore="#00ffff" back="#606060" style="5" />
  <assign name="xce:DialogItemButtonColorDisabled" cfore="#7" cback="#8" fore="#c0c0c0" back="#606060" />


  <assign name="xce:DialogItemEditColor" cfore="#8" cback="#0" fore="#000000" back="#808080" />
  <assign name="xce:DialogItemEditColorDisabled" cfore="#7" cback="#0" fore="#c0c0c0" back="#808080" />
  <assign name="xce:DialogItemEditColorFocused" cfore="#f" cback="#0" fore="#ffffff" back="#808080" />
  <assign name="xce:DialogItemEditColorPair" cfore="#a" cback="#0" fore="#42ae8b" back="#808080" />
  <assign name="xce:DialogItemEditColorError" cfore="#c" cback="#0" fore="#9314ff" back="#808080" />
  <assign name="xce:DialogItemEditColorKeyword" cfore="#b" cback="#0" fore="#303030" back="#808080" style="1" />
  <assign name="xce:DialogItemEditSelection" cfore="#0" cback="#3" fore="#000000" back="#c06000" />
  <assign name="xce:DialogItemEditSelectionFocused" cfore="#f" cback="#3" fore="#ffffff" back="#ff8000" />

  <assign name="xce:DialogItemListBoxColor" cfore="#8" cback="#0" fore="#000000" back="#808080" />
  <assign name="xce:DialogItemListBoxColorDisabled" cfore="#7" cback="#0" fore="#c0c0c0" back="#808080" />
  <assign name="xce:DialogItemListBoxColorFocused" cfore="#f" cback="#0" fore="#ffffff" back="#808080" />
  <assign name="xce:DialogItemListBoxSelection" cfore="#3" cback="#0" fore="#000000" back="#c06000" />
  <assign name="xce:DialogItemListBoxSelectionFocused" cfore="#3" cback="#f" fore="#ffffff" back="#ff8000" />
  <assign name="xce:DialogItemListBoxSelectionHighlighted" cfore="#b" cback="#0" fore="#00d0d0" back="#face87" />
  <assign name="xce:DialogItemListBoxSelectionFocusedHighlighted" cfore="#b" cback="#f" fore="#00ffff" back="#face87" />
</hrd>
